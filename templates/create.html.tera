{% extends "base"%}
{% block content %}
    <h1> Create a new post </h1>
    <form action="/upload" method="POST">
        <input type="text" name="title" placeholder="Some Text">
        <input type="file" name="image" accept="image/png, image/jpg, text/txt">
        <button type="submit">Submit</button>
    </form>
{% endblock content %}